@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<meta http-equiv="content-type" content="text/html;charset=UTF-8" /><!-- /Added by HTTrack -->
<head>
    <meta charset="utf-8" />

    <title>Help Disk | Dashboard</title>
    <meta name="description" content="Latest updates and statistic charts">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no">

    <!--begin::Web font -->
    <script src="~/Content/webfont/webfont.js"></script>
    <script>
        WebFont.load({
            google: { "families": ["Poppins:300,400,500,600,700", "Roboto:300,400,500,600,700"] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <!--end::Web font -->
    <!--begin::Global Theme Styles -->
    <link href="~/Content/assets/vendors/base/vendors.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/assets/demo/demo5/base/style.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Global Theme Styles -->
    <!--begin::Page Vendors Styles -->
    <link href="~/Content/assets/vendors/custom/fullcalendar/fullcalendar.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <!--end::Page Vendors Styles -->
    <link rel="shortcut icon" href="~/Content/Logo/Round_logo.ico" />
</head>
<!-- end::Head -->
<!-- begin::Body -->
<body class="m-page--wide m-header--fixed m-header--fixed-mobile m-footer--push m-aside--offcanvas-default">



    <!-- begin:: Page -->
    <div class="m-grid m-grid--hor m-grid--root m-page">
        <div class="m-grid__item m-grid__item--fluid m-wrapper">
            <!-- END: Subheader -->
            <div class="m-content">
                <!--Begin::Section-->

                <div class="m-portlet">
                    <div class="container">

                        <div class="panel panel-primary">
                            <div class="panel-heading text-center">
                                <h2>Welcome to IT & Network Department Query Portal</h2>
                                @{
                                    using (Html.BeginForm("SignOut", "Login", FormMethod.Post))
                                    {
                                        <button class=" btn m-btn--pill btn-secondary m-btn m-btn--custom m-btn--label-brand m-btn--bolder"> Logout</button>
                                        // <a href="#" class="btn m-btn--pill btn-secondary m-btn m-btn--custom m-btn--label-brand m-btn--bolder">Logout</a>
                                    }
                                }
                            </div>
                            <div class="panel-body">
                                <div class="row m-row--no-padding ">
                                    
                                    @*<div class="col-xl-6" style="padding:0px 5px;">
                                        <div class="panel panel-success">
                                            <div class="panel-heading text-center" style="">Your Previous Problem</div>
                                            <div class="panel-body">
                                                <table class="table table-bordered">
                                                    <thead>
                                                        <tr>
                                                            <th>Sr#</th>
                                                            <th>Name</th>
                                                            <th>Assign Time</th>
                                                            <th>Office/Room</th>
                                                            <th>Status</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>*@

                                    <div class="col-xl-12" style="padding:0px 200px;">
                                        <div class="panel panel-success">
                                            <div class="panel-heading text-center" style="">Submit Form</div>
                                            <div class="panel-body">
                                                <form class="form-horizontal">

                                                    <div class="form-group">
                                                        <label class="control-label col-sm-2" for="IssueNo"></label>
                                                        <label class="control-label col-sm-2" id="IssueNo"></label>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="control-label col-sm-2" for="EmployeeID">Emp ID:</label>
                                                        <div class="col-sm-10">
                                                            <input type="number" class="form-control" id="EmployeeID" placeholder="Enter Employee ID" />
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="control-label col-sm-2" for="Description">Problem:</label>
                                                        <div class="col-sm-10">
                                                            <textarea class="form-control" id="Description" placeholder="Enter Problem"></textarea>
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="control-label col-sm-2" for="Office">Office/Room:</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" class="form-control" id="Office" placeholder="Enter Office/Room" />
                                                        </div>
                                                    </div>
                                                    @*<div class="form-group">
                                                        <label class="control-label col-sm-2" for="Remarks">Remarks:</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" class="form-control" id="Remarks" placeholder="Remarks" />
                                                        </div>
                                                    </div>*@
                                                    <div class="form-group">
                                                        <div class="col-sm-offset-2 col-sm-10">
                                                            <button onclick="SaveItem()" type="button" class="btn btn-success btn-sm">Submit</button>
                                                            <button onclick="Reset()"  type="reset" id="reset" class="btn btn-warning btn-sm">Reset</button>
                                                            <button type="button" id="Delete" class="btn btn-danger btn-sm">Delete</button>
                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>
                <!--End::Section-->
            </div>
        </div>
        <!-- end::Body -->
        <!-- begin::Footer -->
        <footer class="m-grid__item m-footer ">
            <div class="m-container m-container--responsive m-container--xxl m-container--full-height m-page__container">
                <div class="m-footer__wrapper">
                    <div class="m-stack m-stack--flex-tablet-and-mobile m-stack--ver m-stack--desktop">
                        <div class="m-stack__item m-stack__item--left m-stack__item--middle m-stack__item--last">
                            <span class="m-footer__copyright">
                                @DateTime.Now.Year &copy; FAST NUCES <a href="https://nu.edu.pk" class="m-link">FAST NU</a>
                            </span>
                        </div>
                        <div class="m-stack__item m-stack__item--right m-stack__item--middle m-stack__item--first">
                            @*<ul class="m-footer__nav m-nav m-nav--inline m--pull-right">
                                    <li class="m-nav__item">
                                        <a href="#" class="m-nav__link">
                                            <span class="m-nav__link-text">About</span>
                                        </a>
